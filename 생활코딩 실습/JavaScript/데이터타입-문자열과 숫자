        console.log('alphabet'.toUpperCase());
        document.write(1+1);        //2 - 숫자끼리 더하면 계산 결과를 출력한다.
        document.write("<br>");
        document.write("1"+"1");    //11 - 문자열끼리 더하면 계산을 하지 않고 이어붙인다.
    </script>
